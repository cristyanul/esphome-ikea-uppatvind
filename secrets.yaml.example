# ESPHome Secrets File - RENAME TO secrets.yaml and fill in your values
# Keep this file private and do not commit to version control!

wifi_ssid: "YOUR_WIFI_SSID"
wifi_password: "YOUR_WIFI_PASSWORD"

# Generate a new encryption key using: esphome generate-encryption-key
api_encryption_key: "YOUR_32_BYTE_BASE64_KEY"

# OTA password for wireless updates
ota_password: "your_ota_password"
